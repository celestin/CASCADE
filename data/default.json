{
    "design" : {
        "name" : "<DESIGN>",
        "clk" : null,
        "rst" : null,
        "rst_level" : 0,
        "cpath" : "100ps",
        "nclk" : 1,
        "nshares" : 1,
        "corner" : "max",
        "sources" : {
            "rtl" : ["src/${design:name}.v"],
            "zero" : ["${synthesis:netlist}"],
            "delta" : ["${synthesis:netlist}"],
            "fanout" : ["${synthesis:netlist}"],
            "gln" : ["${synthesis:netlist}"],
            "par" : ["par/${design:name}-${lib_id}.v"]
        }
    },
    "analysis" : {
        "portmap" : "sca/${design:name}.iox",
        "batchsize" : null
    },
    "test" : {
        "id": "${design:name}",
        "ivbits": 0,
        "ivector": "IV",
