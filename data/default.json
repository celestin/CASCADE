        "target": "${test:ovector}",
        "tbfile": "tb/${test:id}_tb.v",
        "tbmodule": "${design:name}_tb",
        "trigger": "TR"
    },
    "simulation" : { 
        "tclk" : 1,
        "precision" : "1ps",
