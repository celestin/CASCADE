    "synthesis" : {
        "tcl" : "net/${design:corner}/syn-${design:name}-${lib_id}.tcl",
        "signifcant_digits" : 3,
        "netlist" : "net/${design:corner}/${design:name}-${lib_id}.v",
        "sdf" : "net/${design:corner}/${design:name}-${lib_id}.sdf",
